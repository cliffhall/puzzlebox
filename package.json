{
  "name": "puzzlebox",
  "version": "1.0.0",
  "scripts": {
    "build": "node esbuild.config.cjs && echo '{\"type\": \"commonjs\"}' > dist/package.json",
    "start": "node dist/index.js",
    "inspector": "npx @modelcontextprotocol/inspector",
    "format": "prettier --write ./src",
